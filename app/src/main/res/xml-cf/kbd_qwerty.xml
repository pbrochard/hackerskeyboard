<Keyboard
	xmlns:android="http://schemas.android.com/apk/res-auto"
	android:keyWidth="11.2%p"
	android:horizontalGap="0px"
	android:verticalGap="@dimen/key_bottom_gap"
>
	<Row>
		<Key
			android:keyLabel="q"
			android:keyEdgeFlags="left" />
		<Key
			android:keyLabel="w" />
		<Key
			android:keyLabel="e" />
		<Key
			android:keyLabel="r" />
		<Key
			android:keyLabel="u"
			android:horizontalGap="11.2%p" />
		<Key
			android:keyLabel="i" />
		<Key
			android:keyLabel="o" />
		<Key
			android:keyLabel="p"
			android:keyEdgeFlags="right" />
	</Row>
	<Row>
		<Key
			android:keyLabel="a"
			android:keyEdgeFlags="left" />
		<Key
			android:keyLabel="s" />
		<Key
			android:keyLabel="d" />
		<Key
			android:keyLabel="f" />
		<Key
			android:keyLabel="j"
			android:horizontalGap="11.2%p" />
		<Key
			android:keyLabel="k" />
		<Key
			android:keyLabel="l" />
		<Key
			android:keyLabel=";"
			android:keyEdgeFlags="right" />
	</Row>
	<Row>
		<Key
			android:keyLabel="z"
			android:keyEdgeFlags="left" />
		<Key
			android:keyLabel="x" />
		<Key
			android:keyLabel="c" />
		<Key
			android:keyLabel="v" />
		<Key
			android:keyLabel="m"
			android:horizontalGap="11.2%p" />
		<Key
			android:keyLabel="," />
		<Key
			android:keyLabel="." />
		<Key
			android:keyLabel="/"
			android:keyEdgeFlags="right" />
	</Row>
	<Row
		android:rowEdgeFlags="bottom"
	>
		<Key
			android:keyLabel="n"
			android:keyWidth="30%p"
		/>
		<Key
			android:codes="@integer/key_space"
			android:keyIcon="@drawable/sym_keyboard_space"
			android:keyWidth="40%p" />
		<Key
			android:keyLabel="b"
			android:keyWidth="30%p"
			android:keyEdgeFlags="right" />
	</Row>
</Keyboard>
